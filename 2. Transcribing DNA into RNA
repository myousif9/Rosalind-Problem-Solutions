sequence <- "
"
new_sequence <- ""
counter <- 1

for (i in 1:nchar(sequence)) {
  if (substr(sequence,counter,counter) == "T") {
    new_sequence <- paste(new_sequence, "U", sep = "", collapse = NULL)
  } 
  else if (substr(sequence,counter,counter) == "A"){
    new_sequence <- paste(new_sequence, "A", sep = "",  collapse = NULL)
  }
  else if (substr(sequence,counter,counter) == "G"){
    new_sequence <- paste(new_sequence, "G", sep = "",  collapse = NULL)
  }
  else if (substr(sequence,counter,counter) == "C"){
    new_sequence <- paste(new_sequence, "C", sep = "",  collapse = NULL)
  }
  
  counter <- counter + 1
}


