sequence <- "
"
new_sequence <- ""
counter <- nchar(sequence)

for (i in 1:nchar(sequence)) {
  
  if (substr(sequence,counter,counter) == "T") {
        new_sequence <- paste0(new_sequence, "A")
  } 
  
  else if (substr(sequence,counter,counter) == "A"){
       new_sequence <- paste0(new_sequence, "T")
  }
  
  else if (substr(sequence,counter,counter) == "G"){
       new_sequence <- paste0(new_sequence, "C")
  }
  
  else if (substr(sequence,counter,counter) == "C"){
       new_sequence <- paste0(new_sequence, "G")
  }
  
  counter <- counter - 1
}

new_sequence